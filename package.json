{
    "name": "otcs-webreport",
    "displayName": "OTCS WebReport",
    "description": "Provides syntax highlighting for Open Text Web Report Views for MS VSC",
    "version": "0.0.1",
    "publisher": "mfr966",
    "author": {
        "name": "Michael Fromme",
        "email": "mfromme@acm.org"
    },
    "license": "MIT",
    "licenses": [
      {
        "type": "MIT",
        "url": "https://github.com/prantlf/vscode-oscript/blob/master/LICENSE"
      }
    ],
      "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "wrv",
            "aliases": ["WebReport", "wrv"],
            "extensions": ["wrv"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "wrv",
            "scopeName": "source.wrv",
            "path": "./syntaxes/wrv.tmLanguage.json"
        }]
    }
}